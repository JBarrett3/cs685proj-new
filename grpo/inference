                    elif category[0] == 4:
                        # Category 4 is the hardest
                        if re.search(r'\b' + word + r'\b', ' '.join(words)):
                            category_dict[category[0]].append(word)

    # Find the categories with 4 words
    final_categories = {}
    for category, words in category_dict.items():
